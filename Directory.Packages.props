    <PackageVersion Include="FileWatcherEx" Version="2.4.0" />
    <PackageVersion Include="FluentAvaloniaUI" Version="2.0.0-preview7.1" />
    <PackageVersion Include="WindowsShortcutFactory" Version="1.1.0" />
    <PackageVersion Include="WmiLight" Version="3.1.0" />
